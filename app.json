			"adaptiveIcon": {
				"foregroundImage": "./assets/chat.jpeg",
				"backgroundColor": "#f8de7e"
			},
			"package": "com.sijwali.Coinbase"
		},
		"web": {
			"favicon": "./assets/chat.jpeg"
		}
	}
}
